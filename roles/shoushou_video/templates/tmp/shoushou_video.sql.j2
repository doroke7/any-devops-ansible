-- ----------------------------
-- Table structure for cmf_agent
-- ----------------------------
DROP TABLE IF EXISTS `cmf_agent`;
CREATE TABLE `cmf_agent` (
  `id` int(100) NOT NULL AUTO_INCREMENT,
  `agent_login` varchar(50) DEFAULT NULL COMMENT '登录账号是：手机号码',
  `agent_pass` varchar(100) DEFAULT NULL COMMENT '代理 登录密码',
  `last_login_ip` varchar(20) DEFAULT NULL COMMENT '最后登录ip地址',
  `last_login_time` int(12) DEFAULT NULL COMMENT '最后登录时间',
  `agent_level` int(3) DEFAULT '1' COMMENT '等级',
  `superior_id` int(100) DEFAULT '0' COMMENT '上级代理id',
  `status` int(1) DEFAULT '1' COMMENT '代理状态：0关闭 1正常',
  `commission` varchar(100) NOT NULL COMMENT '代理的分成比例%',
  `channel` varchar(100) NOT NULL COMMENT '代理渠道号',
  `channel_agent` varchar(100) NOT NULL COMMENT '代理给的下级渠道号',
  `channel_agent_link` varchar(100) NOT NULL COMMENT '代理邀请渠道码',
  `addtime` int(12) DEFAULT NULL COMMENT '注册时间',
  `buckle_quantity` int(11) NOT NULL COMMENT '扣量',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=38 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC;

-- ----------------------------
-- Table structure for cmf_agent_information
-- ----------------------------
DROP TABLE IF EXISTS `cmf_agent_information`;
CREATE TABLE `cmf_agent_information` (
  `id` int(100) NOT NULL AUTO_INCREMENT COMMENT '身份信息',
  `agent_id` int(100) NOT NULL COMMENT '代理id',
  `email` varchar(100) DEFAULT NULL COMMENT 'Email邮箱',
  `mobile` varchar(20) DEFAULT NULL COMMENT '手机号码',
  `qq` varchar(20) DEFAULT NULL COMMENT 'QQ号',
  `wx` varchar(100) DEFAULT NULL COMMENT '微信号',
  `identity_card` varchar(100) DEFAULT NULL COMMENT '身份证号',
  `name` varchar(100) DEFAULT NULL COMMENT '姓名',
  `pay` varchar(100) DEFAULT NULL COMMENT '支付宝账号',
  `pay_name` varchar(100) DEFAULT NULL COMMENT '收款人姓名',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC;

-- ----------------------------
-- Table structure for cmf_agent_order_log
-- ----------------------------
DROP TABLE IF EXISTS `cmf_agent_order_log`;
CREATE TABLE `cmf_agent_order_log` (
  `id` int(100) NOT NULL AUTO_INCREMENT COMMENT '代理收益用户充值表',
  `order_id` int(100) DEFAULT NULL COMMENT '订单id',
  `channel_link` varchar(100) DEFAULT NULL COMMENT '渠道号',
  `uid` int(100) DEFAULT NULL COMMENT '充值的用户',
  `money` varchar(100) DEFAULT NULL COMMENT '充值的金额（元）',
  `type` int(1) DEFAULT '1' COMMENT '1不扣  2后天扣量',
  `agent_commission` varchar(100) DEFAULT NULL COMMENT '代理提成比例',
  `agent_money` varchar(100) DEFAULT '0' COMMENT '代理收益',
  `addtime` int(11) DEFAULT NULL COMMENT '添加时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=24 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC;

-- ----------------------------
-- Table structure for cmf_agent_statistical
-- ----------------------------
DROP TABLE IF EXISTS `cmf_agent_statistical`;
CREATE TABLE `cmf_agent_statistical` (
  `id` int(100) NOT NULL AUTO_INCREMENT COMMENT '代理每日统计id',
  `uid` int(100) NOT NULL COMMENT '代理id',
  `channel` varchar(100) NOT NULL COMMENT '代理渠道',
  `channel_agent` varchar(100) NOT NULL COMMENT '代理子渠道',
  `money` varchar(255) NOT NULL COMMENT '总充值',
  `registered` int(100) NOT NULL COMMENT '注册安装数',
  `data_time` varchar(100) NOT NULL COMMENT '时间日期r如2018-10-15',
  `commission` varchar(100) NOT NULL COMMENT '收益比例%',
  `earnings` varchar(255) DEFAULT '0' COMMENT '渠道子收益',
  `agent_commission` varchar(255) DEFAULT '0' COMMENT '代理提成',
  `agent_earnings` varchar(100) NOT NULL COMMENT '一级代理提成收益',
  `addtime` int(13) NOT NULL COMMENT '添加时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=723 DEFAULT CHARSET=utf8 COMMENT='代理每日统计表';

-- ----------------------------
-- Table structure for cmf_agent_withdrawal
-- ----------------------------
DROP TABLE IF EXISTS `cmf_agent_withdrawal`;
CREATE TABLE `cmf_agent_withdrawal` (
  `id` int(100) NOT NULL AUTO_INCREMENT,
  `agent_id` int(100) DEFAULT NULL COMMENT '提现代理id',
  `money` varchar(100) DEFAULT '0' COMMENT '提现金额',
  `addtime` int(12) DEFAULT NULL COMMENT '申请时间',
  `status` int(1) DEFAULT '0' COMMENT '状态0审核中1通过2拒绝提现',
  `channel` varchar(100) NOT NULL COMMENT '渠道号',
  `level` int(11) NOT NULL COMMENT '代理级别',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC;

-- ----------------------------
-- Table structure for cmf_app_analyze
-- ----------------------------
DROP TABLE IF EXISTS `cmf_app_analyze`;
CREATE TABLE `cmf_app_analyze` (
  `id` int(100) NOT NULL AUTO_INCREMENT COMMENT '统计日活跃数量id',
  `uid` int(100) NOT NULL COMMENT '用户当天登录的id',
  `day` varchar(20) NOT NULL COMMENT '时间日期例如： 2018-10-27',
  `registered` int(100) NOT NULL DEFAULT '0' COMMENT '统计注册总数',
