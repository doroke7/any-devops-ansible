-- Table structure for cmf_user
-- ----------------------------
DROP TABLE IF EXISTS `cmf_user`;
CREATE TABLE `cmf_user` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `user_type` tinyint(3) unsigned NOT NULL DEFAULT '1' COMMENT '用户类型;1:admin;2:会员',
  `sex` tinyint(2) NOT NULL DEFAULT '1' COMMENT '性别;0:保密,1:男,2:女',
  `birthday` int(11) NOT NULL DEFAULT '0' COMMENT '生日',
  `last_login_time` int(11) NOT NULL DEFAULT '0' COMMENT '最后登录时间',
  `score` int(11) NOT NULL DEFAULT '0' COMMENT '用户积分',
  `coin_system` int(11) DEFAULT '0' COMMENT '系统赠送的金币',
  `coin` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '充值的金币',
  `create_time` int(11) NOT NULL DEFAULT '0' COMMENT '注册时间',
  `user_status` tinyint(3) unsigned NOT NULL DEFAULT '2' COMMENT '用户状态;0:禁用,1:正常,2:未验证',
  `user_login` varchar(60) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '用户名',
  `user_pass` varchar(64) NOT NULL DEFAULT '' COMMENT '登录密码;cmf_password加密',
  `user_nickname` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '用户昵称',
  `user_email` varchar(100) NOT NULL DEFAULT '' COMMENT '用户登录邮箱',
  `user_url` varchar(100) NOT NULL DEFAULT '' COMMENT '用户个人网址',
  `avatar` varchar(255) NOT NULL DEFAULT '' COMMENT '用户头像',
  `signature` varchar(255) NOT NULL DEFAULT '' COMMENT '个性签名',
  `last_login_ip` varchar(15) NOT NULL DEFAULT '' COMMENT '最后登录ip',
  `user_activation_key` varchar(60) NOT NULL DEFAULT '' COMMENT '激活码',
  `mobile` varchar(20) NOT NULL DEFAULT '' COMMENT '用户手机号',
  `more` text COMMENT '扩展属性',
  `token` varchar(100) NOT NULL COMMENT 'token值',
  `address` varchar(50) DEFAULT '0' COMMENT '用户登录地址',
  `level` int(20) DEFAULT '1' COMMENT '级别id',
  `status` tinyint(4) NOT NULL DEFAULT '1' COMMENT '0离线状态1在线状态2空闲状态3忙碌状态',
  `teacher` int(1) DEFAULT '0' COMMENT '0新人1师傅2徒弟',
  `income` int(11) NOT NULL COMMENT '收益',
  `income_total` int(11) NOT NULL COMMENT '总收益',
  `last_remove_message_time` int(11) NOT NULL COMMENT '上次清空消息时间',
  `fabulous` int(11) NOT NULL COMMENT '被赞次数',
  `is_reg_perfect` tinyint(4) NOT NULL COMMENT '是否已经完善过资料0没有1已完善',
  `custom_video_charging_coin` int(11) NOT NULL COMMENT '自定义视频计时收费价格',
  `is_open_do_not_disturb` tinyint(4) NOT NULL COMMENT '是否开启免打扰',
  `login_type` tinyint(4) NOT NULL COMMENT '0手机登录,1微信登录',
  `plat_id` varchar(50) NOT NULL COMMENT '第三方授权ID',
  `reference` tinyint(4) NOT NULL COMMENT '是否推荐1推荐，0不推荐',
  `last_change_name_time` int(11) NOT NULL COMMENT '最后一次修改昵称时间',
  `is_use_free_time` tinyint(4) NOT NULL COMMENT '免费通话是否试用',
  `is_online` tinyint(4) NOT NULL COMMENT '是否在线 0 不在线 1 在线',
  `vip_end_time` int(11) NOT NULL COMMENT 'VIP到期时间',
  `link_id` int(11) NOT NULL DEFAULT '0' COMMENT '渠道号',
  `invitation_coin` decimal(50,2) NOT NULL DEFAULT '0.00' COMMENT '备注邀请收益（元）',
  `sort` int(50) NOT NULL DEFAULT '0' COMMENT '排序由大到小排序',
  `is_auth` tinyint(4) NOT NULL DEFAULT '0' COMMENT '1认证0未认证',
  `alipay_account` varchar(50) NOT NULL COMMENT '支付宝账号',
  `alipay_account_name` varchar(50) NOT NULL COMMENT '支付宝账号名',
  `host_bay_video_proportion` varchar(10) NOT NULL DEFAULT '0' COMMENT '一对一购买视频分成比例',
  `host_bay_phone_proportion` varchar(10) NOT NULL DEFAULT '0' COMMENT '购买私照分成比例',
  `host_bay_gift_proportion` varchar(10) NOT NULL DEFAULT '0' COMMENT '赠送礼物分成比例',
  `host_one_video_proportion` varchar(10) NOT NULL DEFAULT '0' COMMENT '一对一视频通话',
  `host_direct_messages` varchar(10) NOT NULL DEFAULT '0' COMMENT '私信消息分成比例',
  `device_uuid` varchar(255) NOT NULL COMMENT '设备ID',
  `host_guardian_proportion` varchar(10) NOT NULL DEFAULT '0' COMMENT '守护分成比例',
  `invite_buckle_probability` int(11) NOT NULL DEFAULT '0' COMMENT '邀请用户绑定扣量概率',
  `invite_buckle_recharge_probability` int(11) NOT NULL DEFAULT '0' COMMENT '邀请充值扣单概率',
  `host_turntable_ratio` varchar(10) NOT NULL DEFAULT '0' COMMENT '转盘分成比例',
  `province` varchar(50) NOT NULL DEFAULT '' COMMENT '省份',
  `city` varchar(50) NOT NULL DEFAULT '',
  `is_open_auto_see_hi` tinyint(4) NOT NULL DEFAULT '0' COMMENT '是否打开自动打招呼功能',
  `wx_number` varchar(50) NOT NULL COMMENT '微信号码',
  `qq_number` varchar(50) NOT NULL COMMENT 'qq号码',
  `phone_number` varchar(50) NOT NULL COMMENT '手机号码',
  `wx_price` int(10) NOT NULL COMMENT '微信价格',
  `qq_price` int(10) NOT NULL COMMENT 'qq价格',
  `phone_price` int(10) NOT NULL COMMENT '手机号码价格',
  `orderno` int(100) NOT NULL DEFAULT '0' COMMENT '推荐排序',
  `shielding_time` int(11) NOT NULL,
  `login_way` int(1) NOT NULL DEFAULT '1' COMMENT '1手机号 2qq 3微信',
  `lat` varchar(50) NOT NULL,
  `lng` varchar(50) NOT NULL,
  `height` varchar(10) NOT NULL,
  `weight` varchar(10) NOT NULL,
  `constellation` varchar(20) NOT NULL,
  `image_label` varchar(50) NOT NULL,
  `introduce` varchar(100) NOT NULL,
  `reward_money` varchar(50) NOT NULL DEFAULT '0' COMMENT '转换的现金',
  `reward_coin` int(11) NOT NULL DEFAULT '0' COMMENT '奖励金币',
  `invite_num` int(11) NOT NULL DEFAULT '0' COMMENT '邀请人数',
  `invite_coin` int(11) NOT NULL DEFAULT '0' COMMENT '邀请奖励金额',
  `reward_total` int(11) NOT NULL DEFAULT '0' COMMENT '总奖励',
  PRIMARY KEY (`id`),
  KEY `user_login` (`user_login`),
  KEY `user_nickname` (`user_nickname`)
) ENGINE=InnoDB AUTO_INCREMENT=200634 DEFAULT CHARSET=utf8mb4 COMMENT='用户表';

