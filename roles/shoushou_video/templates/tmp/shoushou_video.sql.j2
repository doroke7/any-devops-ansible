  `is_audio` int(5) NOT NULL COMMENT '0：包含音频 1包含音频',
  `audio_file` varchar(255) NOT NULL COMMENT '音频路径',
  `publish_time` varchar(15) NOT NULL COMMENT '发布时间',
  `msg_content` text CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT '动态文本内容',
  `uid` int(50) NOT NULL COMMENT 'uid',
  `video_url` varchar(255) NOT NULL DEFAULT '' COMMENT '视频地址',
  `cover_url` varchar(255) NOT NULL COMMENT '视频封面地址',
  `city` varchar(50) NOT NULL COMMENT '城市',
  `lat` varchar(100) NOT NULL,
  `lng` varchar(100) NOT NULL,
  `duration` varchar(20) NOT NULL COMMENT '语音长度',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for cmf_bzone_images
-- ----------------------------
DROP TABLE IF EXISTS `cmf_bzone_images`;
CREATE TABLE `cmf_bzone_images` (
  `id` int(5) NOT NULL AUTO_INCREMENT,
  `zone_id` int(100) NOT NULL COMMENT '动态id',
  `img` varchar(255) NOT NULL COMMENT '图片地址',
  `addtime` varchar(20) NOT NULL COMMENT '添加时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for cmf_bzone_like
-- ----------------------------
DROP TABLE IF EXISTS `cmf_bzone_like`;
CREATE TABLE `cmf_bzone_like` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(50) NOT NULL COMMENT '点赞人id',
  `addtime` varchar(20) NOT NULL COMMENT '添加时间',
  `zone_id` int(50) NOT NULL COMMENT '动态id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for cmf_bzone_reply
-- ----------------------------
DROP TABLE IF EXISTS `cmf_bzone_reply`;
CREATE TABLE `cmf_bzone_reply` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `addtime` varchar(15) NOT NULL COMMENT '评论发布时间',
  `body` varchar(500) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL COMMENT '评论内容',
  `uid` int(20) NOT NULL COMMENT '用户id',
  `zone_id` int(50) NOT NULL COMMENT '动态id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for cmf_cash_rule
-- ----------------------------
DROP TABLE IF EXISTS `cmf_cash_rule`;
CREATE TABLE `cmf_cash_rule` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `count` int(11) NOT NULL COMMENT 'create_time',
  `create_time` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for cmf_comment
-- ----------------------------
DROP TABLE IF EXISTS `cmf_comment`;
CREATE TABLE `cmf_comment` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `parent_id` bigint(20) unsigned NOT NULL DEFAULT '0' COMMENT '被回复的评论id',
  `user_id` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '发表评论的用户id',
  `to_user_id` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '被评论的用户id',
  `object_id` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '评论内容 id',
  `create_time` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '评论时间',
  `delete_time` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '删除时间',
  `status` tinyint(3) unsigned NOT NULL DEFAULT '1' COMMENT '状态,1:已审核,0:未审核',
  `type` tinyint(3) unsigned NOT NULL DEFAULT '1' COMMENT '评论类型；1实名评论',
  `table_name` varchar(64) NOT NULL DEFAULT '' COMMENT '评论内容所在表，不带表前缀',
  `full_name` varchar(50) NOT NULL DEFAULT '' COMMENT '评论者昵称',
  `email` varchar(255) NOT NULL DEFAULT '' COMMENT '评论者邮箱',
  `path` varchar(255) NOT NULL DEFAULT '' COMMENT '层级关系',
  `url` text COMMENT '原文地址',
  `content` text COMMENT '评论内容',
  `more` text COMMENT '扩展属性',
  PRIMARY KEY (`id`),
  KEY `comment_post_ID` (`object_id`),
  KEY `comment_approved_date_gmt` (`status`),
  KEY `comment_parent` (`parent_id`),
  KEY `table_id_status` (`table_name`,`object_id`,`status`),
  KEY `createtime` (`create_time`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='评论表';

-- ----------------------------
-- Table structure for cmf_config
-- ----------------------------

## 后台 >> 系统设置 >> 系统配置
DROP TABLE IF EXISTS `cmf_config`;
CREATE TABLE `cmf_config` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '系统设置',
  `code` varchar(255) DEFAULT NULL COMMENT '配置名称',
  `title` varchar(255) DEFAULT NULL COMMENT '标题',
  `group_id` varchar(50) DEFAULT NULL COMMENT '分组名称',
  `val` text COMMENT '配置值',
  `type` tinyint(1) NOT NULL COMMENT '类型',
  `sort` int(11) DEFAULT '0' COMMENT '排序',
  `value_scope` varchar(50) DEFAULT NULL COMMENT '值的范围',
  `title_scope` varchar(255) DEFAULT NULL COMMENT '对应value_scope的中文解释',
  `desc` varchar(255) DEFAULT NULL COMMENT '描述',
  `status` tinyint(4) NOT NULL DEFAULT '1' COMMENT '0是关闭 1是打开',
  PRIMARY KEY (`id`),
  UNIQUE KEY `code` (`code`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=395 DEFAULT CHARSET=gbk ROW_FORMAT=DYNAMIC;

