SET NAMES utf8mb4;
SET FOREIGN_KEY_CHECKS = 0;

-- ----------------------------
-- Table structure for cai_category
-- ----------------------------
DROP TABLE IF EXISTS `cai_category`;
CREATE TABLE `cai_category`  (
  `id` int(11) UNSIGNED NOT NULL AUTO_INCREMENT,
  `lotCode` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  `name` varchar(100) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '名称',
  `lottype` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '设置站长推荐下的data-text值，前端用于识别彩种',
  `alias` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  `alias1` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  `alias2` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  `ccdior` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT 'cc编码',
  `cccpw` int(4) NULL DEFAULT 0,
  `caipiaokong` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  `video_type` tinyint(3) NULL DEFAULT 0 COMMENT 'videoType：0-暂无视频 1-赛车  2-飞艇  3-时时彩  4-十一选五  5-快三  6-快乐十分  7-快乐十分（广西）  8-快乐8  9-幸运农场  10-PC蛋蛋  11-福彩双色球  12-福彩3排 ',
  `type` int(11) NULL DEFAULT NULL COMMENT '类型 1-pk拾系列 2-时时彩系列 3-11选5系列 4-快3系列 5-广东快乐十分系列 6：全国彩 （三字系列）7：快乐8系列（基诺型彩） 8：广西快乐十分系列 9:PC蛋蛋',
  `frequency` int(11) NULL DEFAULT 0 COMMENT '频率（单位s）',
  `startTime` int(11) NULL DEFAULT 0 COMMENT '开始的时间',
  `totalCount` int(11) NULL DEFAULT 0 COMMENT '一天内总期数',
  `drawCount` int(11) NULL DEFAULT 0 COMMENT '已开期数',
  `database` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '数据库',
  `issue` bigint(20) NULL DEFAULT 0 COMMENT '下期开奖期数',
  `openTime` bigint(20) NULL DEFAULT 0 COMMENT '下期开奖时间',
  `restTime` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT '' COMMENT '中断时间段',
  `delay_time` int(11) NULL DEFAULT 0 COMMENT '实际开奖延迟秒数',
  `curr_issue_time` timestamp(0) NULL DEFAULT NULL COMMENT '当前期开奖时间',
  `planTime` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT '' COMMENT '购彩计划时间段',
  `sideScore` tinyint(3) NULL DEFAULT 0 COMMENT '两面积分',
  `status` tinyint(3) NULL DEFAULT 1 COMMENT '1:开启 2：关闭',
  `official` tinyint(1) NULL DEFAULT 0 COMMENT '是否官彩 0表示否，1表示是，3表示是相同彩种，调用的开奖记录应该为link_id对应彩种的记录',
  `link_id` int(11) NULL DEFAULT 0 COMMENT '关联ID',
  `hot` tinyint(1) NOT NULL DEFAULT 0 COMMENT '是否有hot标识  1：有',
  `hot_type` varchar(10) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL DEFAULT '0' COMMENT '目前没用',
  `is_caipiao` tinyint(3) NULL DEFAULT 0 COMMENT '是否彩票大厅推荐（已放弃使用）',
  `is_zhanzhang` tinyint(3) NULL DEFAULT 0 COMMENT '是否站长推荐',
  `is_liangmian` tinyint(3) NULL DEFAULT 0 COMMENT '是否两面推荐',
  `is_hot` tinyint(3) NULL DEFAULT 0 COMMENT '是否为热门彩',
  `is_often` tinyint(3) NULL DEFAULT 0 COMMENT '是否为高频彩',
  `is_quanguo` tinyint(3) NULL DEFAULT 0 COMMENT '是否全国彩',
  `icon` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '彩种图片',
  `video_thumb` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '彩种开奖视频图片',
  `home_video` tinyint(1) NULL DEFAULT 0 COMMENT '是否首页视频 1是 0否',
  `content` longtext CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT '玩法规则',
  `sort` int(5) NULL DEFAULT 0 COMMENT '首页彩种推荐热门彩排序 大的排在前面',
  `video_sort` int(5) NULL DEFAULT 0 COMMENT '视频推荐排序',
  `rangeTimes` json NULL,
  `days` json NULL,
  `holidays` json NULL,
  `format` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 10181 CHARACTER SET = utf8 COLLATE = utf8_general_ci COMMENT = '类别' ROW_FORMAT = Compact;

